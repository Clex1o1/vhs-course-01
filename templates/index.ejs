<div class="card">
  <div class="card-body">
    <h1 class="card-title"><%= title %></h1>
    <p class="card-text"><%= message %></p>

    <h3>User Information</h3>
    <p>Name: <%= user.name %></p>
    <p>Age: <%= user.age %></p>

    <h3>Items List</h3>
    <ul class="list-group">
      <% items.forEach(function(item) { %>
      <li class="list-group-item"><%= item %></li>
      <% }); %>
    </ul>
  </div>
</div>
